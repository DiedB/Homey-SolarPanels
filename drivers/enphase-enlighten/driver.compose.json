{
  "name": {
    "en": "Enphase Enlighten"
  },
  "class": "solarpanel",
  "capabilities": ["measure_power", "meter_power"],
  "images": {
    "large": "{{driverAssetsPath}}/images/large.jpg",
    "small": "{{driverAssetsPath}}/images/small.jpg"
  },
  "connectivity": ["cloud"],
  "platforms": ["local", "cloud"],
  "pair": [
    {
      "id": "login_oauth2",
      "template": "login_oauth2"
